# Frontend (Vite) environment variables
# Copy to .env.local and fill with your values

# Azure Functions API base URL
VITE_API_BASE_URL=http://localhost:7071

# Auth0 settings (no protocol in domain)
VITE_AUTH0_DOMAIN=your-tenant-region.auth0.com
VITE_AUTH0_CLIENT_ID=your-spa-client-id
VITE_AUTH0_AUDIENCE=your-api-identifier

# Optional: custom roles claim if youâ€™ve added one (leave empty otherwise)
# e.g. https://yourdomain.example/claims/roles
VITE_AUTH0_ROLES_CLAIM=

# Inventory service selection
# VITE_INVENTORY_SERVICE=http
# VITE_INVENTORY_BASE_URL=http://localhost:7071
# VITE_USE_SEED_DATA=false
